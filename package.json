{
  "name": "bin-cli-next",
  "version": "0.0.1",
  "keywords": [
    "bin-ui",
    "bin",
    "ui",
    "vue",
    "vue",
    "components"
  ],
  "author": "wang bin",
  "files": [
    "lib",
    "es",
    "src"
  ],
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "unpkg": "lib/index.js",
  "private": false,
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --config build/webpack.dev.js",
    "build:demo": "rimraf docs && cross-env NODE_ENV=production webpack --config build/webpack.dev.js",
    "build": "yarn clean:lib && yarn build:esm-bundle && yarn build:lib && yarn build:lib-full && yarn build:utils && yarn build:style",
    "clean:lib": "rimraf lib && rimraf es",
    "build:esm-bundle": "rollup --config ./build/rollup.config.bundle.js",
    "build:lib": "cross-env LIBMODE=core webpack --config ./build/webpack.config.js",
    "build:lib-full": "cross-env LIBMODE=full webpack --config ./build/webpack.config.js",
    "build:utils": "cross-env BABEL_ENV=utils babel src/utils --extensions .ts --out-dir lib/utils",
    "build:style": "gulp --gulpfile build/build-style.js",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts"
  },
  "dependencies": {},
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@lerna/project": "^3.21.0",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@vue/babel-plugin-jsx": "^1.0.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/component-compiler-utils": "^3.2.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "eslint": "^7.7.0",
    "eslint-plugin-vue": "^7.0.0-0",
    "file-loader": "^6.0.0",
    "file-save": "^0.2.0",
    "highlight.js": "^10.5.0",
    "html-webpack-plugin": "^4.3.0",
    "import-from": "^3.0.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.0",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "mini-css-extract-plugin": "^0.11.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.2",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "rollup-plugin-vue": "^6.0.0-beta.11",
    "style-loader": "^1.2.1",
    "stylus": "^0.54.8",
    "stylus-loader": "3.0.2",
    "throttle-debounce": "2.3.0",
    "transliteration": "^2.2.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.1",
    "vue": "^3.0.5",
    "vue-loader": "^16.1.2",
    "vue-router": "^4.0.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
